package net.thesahara.game.logic;

import java.util.concurrent.TimeUnit;

/**
 *The Sahara - Time.java
 *@author Greg
 *
 */
public class Time implements Runnable{

	private static boolean isRunning;
	@Override
	public void run(){
		int i;
		for(i = 0; i < 1; i++){
			TimeHandler.increaseTime(1);
		}
		
	}
	

}
